{
    "name": "wramp-syntax",
    "displayName": "WRAMP Syntax Highlighting",
    "description": "WRAMP Assembly syntax highlighting",
    "version": "1.0.0",
    "publisher": "wanduow",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wramp",
            "aliases": ["WRAMP Assembly", "wramp"],
            "extensions": [".s",".S"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wramp",
            "scopeName": "source.wramp",
            "path": "./syntaxes/wramp.tmLanguage.json"
        }]
    }
}